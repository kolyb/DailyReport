@page
@model DailyReport.WebLayer.Pages.Workplace.DeleteWorkplaceModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<form method="post">
    <div asp-validation-summary="All"></div>
    <div>
        <label>ID</label>
        <div>
            <input asp-for="Id" />
            <span asp-validation-for="Id"></span>
        </div>
    </div>
    <div>
        <label>WithoutWorkplace</label>
        <div>
            <input asp-for="WithoutWorkplaceId" />
            <span asp-validation-for="WithoutWorkplaceId"></span>
        </div>
    </div>
    <div>
        <label>Description</label>
        <div>
            <input asp-for="Description" />
            <span asp-validation-for="Description"></span>
        </div>
    </div>
    <div>
        <label>City</label>
        <div>
            <input asp-for="AdressCity" />
            <span asp-validation-for="AdressCity"></span>
        </div>
    </div>
    <div>
        <label>Street</label>
        <div>
            <input asp-for="AdressStreet" />
            <span asp-validation-for="AdressStreet"></span>
        </div>
    </div>
    <div>
        <label>House</label>
        <div>
            <input asp-for="AdressHouse" />
            <span asp-validation-for="AdressHouse"></span>
        </div>
    </div>
    <div>
        <input type="submit" style="background-color:red"value="Delete" class="btn btn-info btn-md" />
        &nbsp;
        <input asp-page-handler="Cancel" style="background-color:green"type="submit" value="Refuse" class="btn btn-info btn-md" />
    </div>
</form>
<div>
    <a asp-controller="Home" asp-action="Index">Back</a>
</div>
